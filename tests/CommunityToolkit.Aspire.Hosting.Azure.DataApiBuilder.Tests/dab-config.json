{
  "$schema": "https://github.com/Azure/data-api-builder/releases/download/v0.9.7/dab.draft.schema.json",
  "data-source": {
    "database-type": "mssql",
    "connection-string": "missing-on-purpose"
  },
  "runtime": {
    "host": {
      "mode": "development"
    }
  },
  "entities": {
    "Actor": {
      "source": {
        "object": "[dbo].[Actor]",
        "type": "table"
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [ "*" ]
        }
      ]
    }
  }
}